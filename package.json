{
  "name": "elevpc-dashboard",
  "description": "Dashboard for elevpc",
  "version": "1.0.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "deploy": "npm test && now --target production",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "cleanup": "rm -rf .next && rm -rf out",
    "now-build": "next build"
  },
  "keywords": [
    "dashboard",
    "status",
    "Telemark fylkeskommune",
    "elevpc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/elevpc-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/elevpc-dashboard/issues"
  },
  "homepage": "https://github.com/telemark/elevpc-dashboard#readme",
  "devDependencies": {
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.6",
    "nyc": "15.1.0",
    "standard": "14.3.1"
  },
  "dependencies": {
    "axios": "0.19.0",
    "next": "9.0.4",
    "next-offline": "4.0.5",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
